        "        fp16=True,\n",
        "        logging_dir=\"./logs\",\n",
        "        logging_steps=10,\n",
        "        save_strategy=\"steps\",\n",
        "        save_steps=50,\n",
        "        eval_strategy=\"steps\",\n",
