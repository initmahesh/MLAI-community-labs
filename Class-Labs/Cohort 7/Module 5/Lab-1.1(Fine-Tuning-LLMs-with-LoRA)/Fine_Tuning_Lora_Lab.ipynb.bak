        "\n",
        "    # Extract only the generated part\n",
        "    generated_part = response.split(prompt)[-1].strip()\n",
        "\n",
        "    # Look for patterns like \"Answer:\" and extract just that part\n",
        "    if \"Answer:\" in generated_part:\n",
        "        first_answer = generated_part.split(\"Answer:\")[1].strip()\n",
        "        # If there's another question after the answer, only keep the answer\n",
        "        if \"Question:\" in first_answer:\n",
        "            first_answer = first_answer.split(\"Question:\")[0].strip()\n",
        "        return first_answer\n",
        "\n",
        "    # Remove repetitions from the response\n",
        "    # This pattern finds cases where the same sentence is repeated\n",
        "    clean_response = remove_repetitions(generated_part)\n",
        "\n",
        "    # Ensure the response is concise\n",
        "    if len(clean_response) > 150:\n",
        "        clean_response = clean_response[:500]\n",
        "\n",
        "    return clean_response\n",
        "\n",
        "def remove_repetitions(text):\n",
        "    \"\"\"\n",
        "    Remove repeated sentences and phrases from the generated text\n",
        "    \"\"\"\n",
        "    # Split into sentences\n",
        "    sentences = re.split(r'(?<=[.!?])\\s+', text)\n",
        "\n",
        "    # Remove duplicates while preserving order\n",
        "    unique_sentences = []\n",
        "    for sentence in sentences:\n",
        "        if sentence and sentence not in unique_sentences:\n",
        "            unique_sentences.append(sentence)\n",
        "\n",
        "    # Join back into a single string\n",
        "    return ' '.join(unique_sentences)\n",
        "\n",
        "def analyze_data_deletion_provision(questions):\n",
        "    \"\"\"\n",
        "    Specifically analyze data deletion provisions\n",
        "    \"\"\"\n",
        "    deletion_keywords = [\n",
        "        'data deletion',\n",
        "        'customer data',\n",
        "        'terminate',\n",
        "        'termination',\n",
        "        'delete',\n",
        "        'destruction',\n",
        "        'end of agreement'\n",
        "    ]\n",
        "\n",
        "    relevant_parts = find_relevant_clauses(questions, deletion_keywords)\n",
        "    print(relevant_parts);\n",
        "\n",
        "    question = \"Does this contract has a provision that mentions that customer data will be deleted upon request by customer or after closure or termination of agreement for whatsoever reason?\"\n",
        "\n",
        "    # Updated prompt to guide the model toward more specific and concise responses\n",
        "    prompt = f\"\"\"\n",
        "    Analyze these contract excerpts for data deletion provisions:\n",
        "    {' '.join(relevant_parts)}\n",
        "\n",
        "    {question}\n",
        "\n",
        "    Respond with a single \"Yes\" or \"No\" followed by one brief sentence explanation.\n",
        "    \"\"\"\n",
        "\n",
        "    print(\"\\n\" + \"=\" * 50)\n",
        "    print(\"Testing LoRA Model\".center(50))\n",
        "    print(\"=\" * 50 + \"\\n\")\n",
        "\n",
        "    # Print question in boxen\n",
        "    print(boxen(\n",
        "        question,\n",
        "        title=\"Question\",\n",
        "        padding=1,\n",
        "        margin=1,\n",
        "        color=\"blue\"\n",
        "    ))\n",
        "\n",
        "    # Generate and print response using LoRA model\n",
        "    with torch.cuda.amp.autocast(enabled=torch.cuda.is_available()):\n",
        "        # Load the LoRA model for testing\n",
        "        lora_model = load_lora_model(model, \"./phi-1_5-lora-msa-final\")\n",
        "        # Generate the response using the LoRA model and tokenizer\n",
        "        lora_response = generate_improved_response(prompt, lora_model, tokenizer)\n",
        "\n",
        "    # Print the LoRA model's response\n",
        "    print(boxen(\n",
        "        lora_response,\n",
        "        title=\"LoRA Model Response\",\n",
        "        padding=1,\n",
        "        margin=1,\n",
        "        color=\"green\"\n",
        "    ))\n",
        "\n",
        "    return lora_response\n",
        "\n",
        "# Function to print a header with centered text and equal signs around it\n",
        "def print_header(text):\n",
        "    print(\"\\n\" + \"=\" * 60)\n",
        "    print(text.center(60))\n",
        "    print(\"=\" * 60 + \"\\n\")\n",
        "\n",
        "# Main execution\n",
        "# Assume model and tokenizer are already loaded\n",
        "\n",
        "# Use train_file_path instead of train_dataset\n",
        "with open(train_file_path, \"r\") as f:\n",
        "    questions = [json.loads(line) for line in f]\n",
        "\n",
        "# Clear memory cache before analysis\n",
        "gc.collect()  # Garbage collection\n",
        "torch.cuda.empty_cache()  # Clear GPU memory cache\n",
        "\n",
        "# Analyze data deletion provisions and store the result\n",
        "result = analyze_data_deletion_provision(questions)"
      ]
    },
