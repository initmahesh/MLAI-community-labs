        "# Set up the trainer\n",
        "from transformers import Trainer, TrainingArguments, DataCollatorForLanguageModeling\n",
        "\n",
        "# Disable wandb integration by setting report_to to \"none\"\n",
        "output_dir = \"./phi-1_5-finetune-msa\"\n",
